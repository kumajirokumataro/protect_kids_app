<h1><%=t('view.login')%></h1>
<%= form_with(model: User.new, url: sessions_path, local: true) do |f| %>
  <%# model: User.new %>
  <%= f.label :email %>
  <%= f.email_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit "Log in" %>
<% end %>

<h3><%=t('view.guest')%><%=t('view.login')%></h1>
<%= form_with(scope: :guestsession, url: guestsessions_path ) do |f| %>
  <%= f.submit "Log in" %>
<% end %>

